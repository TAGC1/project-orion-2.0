<?xml version='1.0' encoding='utf-8'?>
<widget version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <access origin="*" />
  
  <feature name="Calendar">
    <param name="ios-package" value="Calendar"/>
  </feature>

  <feature name="FirebaseAnalytics">
    <param name="ios-package" value="FirebaseAnalyticsPlugin"/>
    <param name="onload" value="$ANALYTICS_COLLECTION_ENABLED"/>
  </feature>

  <feature name="FirebaseAuthentication">
    <param name="ios-package" value="FirebaseAuthenticationPlugin"/>
    <param name="onload" value="false"/>
  </feature>

  <feature name="FirebaseMessaging">
    <param name="ios-package" value="FirebaseMessagingPlugin"/>
    <param name="onload" value="true"/>
  </feature>

  <feature name="Device">
    <param name="ios-package" value="CDVDevice"/>
  </feature>

  
</widget>